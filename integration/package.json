{
  "name": "integration-test",
  "version": "1.0.0",
  "description": "App for integration tests",
  "scripts": {
    "clean": "rimraf aot/ dist/ node_modules/micro-dash/",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "build:aot": "node build.js",
    "serve": "lite-server -c=bs-config.json",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "serve:e2e-aot": "lite-server -c bs-config.e2e-aot.json",
    "prestart": "yarn run build",
    "start": "concurrently \"yarn run build:watch\" \"yarn run serve\"",
    "pree2e": "yarn run build:e2e && yarn run build",
    "e2e": "concurrently \"yarn run serve:e2e\" \"yarn run protractor\" --kill-others --success first",
    "pree2e:aot": "yarn run build:e2e && yarn run build:aot",
    "e2e:aot": "concurrently \"yarn run serve:e2e-aot\" \"yarn run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "micro-dash": "../dist/",
    "core-js": "^2.4.1",
    "systemjs": "0.19.40"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "concurrently": "^3.4.0",
    "jasmine-core": "~2.4.1",
    "glob": "^7.1.1",
    "lite-server": "^2.2.2",
    "protractor": "~5.1.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.42.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "typescript": "~2.3.0"
  },
  "repository": {}
}
