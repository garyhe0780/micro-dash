{
  "name": "micro-dash",
  "version": "1.0.0",
  "description": "Library QuickStart package.json from the documentation, supplemented with testing support",
  "main": "./bundles/micro-dash.umd.js",
  "module": "./micro-dash.es5.js",
  "es2015": "./micro-dash.js",
  "typings": "./micro-dash.d.ts",
  "author": "",
  "license": "MIT",
  "repository": {},
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf build/* dist/* coverage/*",

    "prebuild": "rimraf build/es5/* build/es2015/* dist/*",
    "build": "tsc -p tsconfig.es5.json && tsc -p tsconfig.es2015.json && node bundle.js",

    "prebuild:test": "rimraf build/spec/*",
    "build:test": "tsc -p tsconfig.spec.json",
    "build:test:watch": "yarn run build:test -- -w",

    "pretest": "yarn run build:test",
    "test": "jasmine --config=jasmine.json",
    "test:watch": "concurrently \"yarn run build:test:watch\" \"nodemon --watch build/spec node_modules/jasmine/bin/jasmine.js --config=jasmine.json\"",

    "preintegration": "yarn run build && cd integration && yarn run clean && yarn install --check-files",
    "integration": "yarn run integration:aot && yarn run integration:jit",
    "integration:jit": "cd integration && yarn run e2e",
    "integration:aot": "cd integration && yarn run e2e:aot",

    "precoverage": "rimraf coverage/* && yarn run build:test",
    "coverage": "istanbul cover node_modules/jasmine/bin/jasmine.js -- --config=jasmine.json",
    "postcoverage": "remap-istanbul -i coverage/coverage.json -t html -o coverage/remapped",

    "presizes": "yarn build && cd sizes && yarn install --check-sizes",
    "sizes": "cd sizes && yarn build",

    "lint": "tslint src/**/*.ts -t verbose",

    "release": "standard-version"
  },
  "devDependencies": {
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@types/jasmine": "^2.5.53",
    "@types/lodash-es": "^4.14.5",
    "@types/node": "^8.0.16",
    "@types/sinon": "^2.3.3",
    "camelcase": "^4.0.0",
    "concurrently": "^3.5.0",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "jasmine": "^2.6.0",
    "lodash-es": "^4.17.4",
    "nodemon": "^1.11.0",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.2",
    "sinon": "^2.3.8",
    "standard-version": "^4.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  }
}
